{
    "language": "ja",
    "title": "僕のドキュメントサイト【GitBook版】",
    "description": "This is Hiroki-IT/tech-notebook_gitbook",
    "styles": {
        "website": "styles/website.css"
    },
    "plugins": [
        "intopic-toc",
        "advanced-emoji",
        "back-to-top-button",
        "-highlight",
        "sunlight-highlighter"
    ],
    "pluginsConfig": {
        "intopic-toc": {
            "selector": "",
            "label": "目次",
            "isCollapsed": true,
            "maxDepth": 3,
            "isScrollspyActive": true
        },
        "sunlight-highlighter": {
            "theme": "dark",
            "lineNumber": true
        }
    }
}
