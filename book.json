{
    "language": "ja",
    "title": "僕のドキュメントサイト【GitBook版】",
    "description": "This is Hiroki-IT/tech-notebook_gitbook",
    "styles": {
        "website": "styles/website.css"
    },
    "plugins": [
        "advanced-emoji",
        "anchors",
        "back-to-top-button",
        "copy-code-button",
        "ga",
        "hide-published-with",
        "-highlight",
        "intopic-toc",
        "prism",
        "-sharing",
        "-lunr",
        "-search", 
        "expandable-chapters",     
        "search-pro-fixed"
    ],
    "pluginsConfig": {
        "intopic-toc": {
            "selector": "",
            "label": "項目一覧",
            "isCollapsed": true,
            "maxDepth": 3,
            "isScrollspyActive": true
        },
        "ga": {
            "token": "UA-127492119-2"
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"
            ],
            "lang": {
                "dockerfile": "docker",
                "elixir": "hcl",
                "sh": "bash",
                "shell": "bash",
                "vue": "javascript"
            }
        }
    }
}
